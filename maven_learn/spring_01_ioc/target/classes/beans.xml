<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="UserDaoImpl" class="com.haige.dao.UserDaoImpl"/>
    <bean id="UserDaoMysqqlImpl" class="com.haige.dao.UserDaoMysqqlImpl"/>

    <bean id="UserServiceImpl" class="com.haige.service.UserServiceImpl">
        <property name="userDao" ref="UserDaoMysqqlImpl"/>
    </bean>
</beans>