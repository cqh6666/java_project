<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <import resource="userbeans.xml"/>
    <bean id="address" class="com.haige.pojo.Address">
        <property name="area" value="广州市"/>
    </bean>
    <bean id="student" class="com.haige.pojo.Student">
        <property name="name" value="haige"/>
        <property name="address" ref="address"/>
        <property name="books">
            <array>
                <value>西游记</value>
                <value>红楼梦</value>
                <value>三国演绎</value>
                <value>水浒传</value>
            </array>
        </property>
        <property name="hobbies">
            <list>
                <value>唱</value>
                <value>跳</value>
                <value>rap</value>
            </list>
        </property>
        <property name="cards">
            <map>
                <entry key="建设银行" value="123123"/>
                <entry key="工商银行" value="3213123"/>
            </map>
        </property>
        <property name="games">
            <set>
                <value>LOL</value>
                <value>王者荣耀</value>
                <value>斗地主</value>
            </set>
        </property>
        <property name="hasWife" value="yes"/>

        <property name="info">
            <props>
                <prop key="学号">202034261231</prop>
                <prop key="性别">男</prop>
                <prop key="姓名">海鸽</prop>
            </props>
        </property>

    </bean>
</beans>